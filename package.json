{
  "name": "@bsweaty/bikologi-aurora",
  "version": "1.0.6",
  "description": "Bikologi Aurora",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bsweaty/bikologi-aurora.git"
  },
  "author": "bsweaty <brian@bikologi.com>",
  "license": "UNLICENSED",
  "engines": {
    "node": "14.12.0"
  },
  "scripts": {
    "build": "node .node-version.js && gulp build",
    "preinstall": "node .node-version.js",
    "prestart": "node .node-version.js",
    "pretest": "node .node-version.js",
    "postinstall": "",
    "start": "gulp watch",
    "test": "jest",
    "test:watch": "node .node-version.js && jest --watch"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build/"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "3",
    "del": "^2.2.2",
    "dotenv": "^8.0.0",
    "es6-promise": "^4.1.0",
    "express": "^4.15.3",
    "express-handlebars": "^6.0.6",
    "fetch-ie8": "^1.5.0",
    "gulp": "^4.0.0",
    "gulp-nodemon": "^2.5.0",
    "gulp4-run-sequence": "^1.0.1",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jquery": "^3.2.1",
    "kill-port": "^2.0.1",
    "query-string": "^4.3.4",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "route-cache": "^0.4.7",
    "run-sequence": "^1.2.2",
    "webpack": "^2.6.1"
  },
  "bugs": {
    "url": "https://docs.bikologi.com"
  },
  "homepage": "https://docs.bikologi.com",
  "keywords": [
    "bikologi"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
